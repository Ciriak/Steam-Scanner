{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Steam Scanner Debug",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceRoot}",
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron",
      "windows": {
        "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron.cmd"
      },
      "program": "${workspaceRoot}/dist/app.js",
      "protocol": "inspector"
    }
  ]
}
